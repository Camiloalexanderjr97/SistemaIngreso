{{>navigations}}

<div class="wrapper d-flex align-items-stretch">
    {{>NavLat}}
    
    <!-- Page Content  -->
    <div id="content" class="p-4 p-md-5 pt-5">
       {{>Message}}
        <div class="limiter">
            <div class="container-table100">
                <div class="wrap-table100">
                    <div class="table100 ver1 m-b-110">
                        <div class="table100-head">
                            <table>
                                <thead>
                                    <tr class="row100 head">
                                        <th class="cell100 column1">Cedula</th>
                                        <th class="cell100 column2">Nombre</th>
                                        <th class="cell100 column3">Edad</th>
                                        <th class="cell100 column3"></th>
                                    </tr>
                                </thead>
                            </table>
                        </div>

                        <div class="table100-body js-pscroll">
                            <table>
                                <tbody>
                                    {{#each usuarios}}
                                    <tr class="row100 body">
                                        <td class="cell100 column1">{{Cedula}}</td>
                                        <td class="cell100 column2">{{Nombre}}</td>
                                        <td class="cell100 column3">{{Edad}}</td>
                                        <td class="cell100 column3">
                                            <div style="inline">
                                                 <a href="/links/Admin/Mostrar/{{Cedula}}" style="padding-right: 30px;">
                                                
                                                    <ion-icon name="eye" size="large"></ion-icon>
                                                </a>
                                                <a href="/links/Admin/Editar/{{Cedula}}" style="padding-right: 30px;">
                                                
                                                    <ion-icon name="pencil" size="large"></ion-icon>
                                                </a>

                                                <a href="/links/Admin/Usuarios/{{Cedula}}">
                                                    <ion-icon name="trash" size="large"></ion-icon>
                                                </a>

                                            </div>
                                        </td>
                                    </tr>
                                    {{/each}}

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{{>script}}




